{
  "name": "yaporouter",
  "version": "0.1.0",
  "description": "Node/TypeScript router for Lambda AWS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist/",
    "compile": "tsc -p .",
    "lint": "npm run clean && tsc --noEmit && eslint \"**/*.{js,ts}\"",
    "lint-fix": "npm run clean && tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "$(npm bin)/mocha --require ts-node/register tests/unit/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FellowSolicist/YapoRouter.git"
  },
  "keywords": [
    "lambda",
    "aws",
    "alb",
    "router"
  ],
  "author": "James Perez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FellowSolicist/YapoRouter/issues"
  },
  "homepage": "https://github.com/FellowSolicist/YapoRouter#readme",
  "dependencies": {
    "http-status-codes": "^1.4.0",
    "path-to-regexp": "^6.1.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@npm/types": "^1.0.1",
    "@types/aws-lambda": "^8.10.59",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.9",
    "@types/uuid": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "mocha": "^7.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
